<div class="hpanel">
  <div class="panel-heading hbuilt row">
    <div class="col-xs-12">
      <h1><%= title %></h1>
    </div>
  </div>

  <div class="panel-body row">
    <div class="col-xs-7">
      <dt>Name</dt>
      <dd><%= name %></dd>

      <dt>Network</dt>
      <dd><%= cell(CardProduct::Cell::Network, model) %></dd>

      <dt>B/P</dt>
      <dd><%= bp %></dd>

      <dt>Type</dt>
      <dd><%= cell(CardProduct::Cell::Type, model) %></dd>

      <dt>Annual fee</dt>
      <dd><%= cell(CardProduct::Cell::AnnualFee, model) %></dd>

      <dt>Currency</dt>
      <dd><%= currency_name %></dd>

      <dt>Bank</dt>
      <dd><%= bank_name %></dd>

      <dt>Wallaby ID</dt>
      <dd><%= wallaby_id %></dd>

      <dt>Shown on survey?</dt>
      <dd><%= shown_on_survey %></dd>

      <dt>Created at</dt>
      <dd><%= created_at %></dd>

      <dt>Last updated at</dt>
      <dd><%= updated_at %></dd>
    </div>

    <div class="col-xs-5">
      <p><%= cell(CardProduct::Cell::Image, model) %></p>

      <p>
        <%= link_to 'Edit', edit_admin_card_product_path(model) %>
        |
        <%= link_to 'Back to all cards', admin_card_products_path %>
      </p>
    </div>
  </div>
</div>
