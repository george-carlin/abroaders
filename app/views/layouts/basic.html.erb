<% layout = cell(Abroaders::Cell::Layout, nil, flash: flash, title: title(yield(:title))) %>
<!DOCTYPE html>
<html>
  <%= layout.head %>
  <body class="<%= layout.body_classes %>">
    <%= layout.navbar %>
    <%= layout.noscript %>
    <%= layout.flash_alerts %>

    <%= yield %>

    <%= layout.footer %>
    <%= layout.third_party_scripts %>
  </body>
</html>
