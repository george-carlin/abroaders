Owner Name: <%= @account.owner_first_name %>
Eligible: <%= @account.owner.eligible? %>
Ready: <%= @account.owner.ready_to_apply? %>

<% if @account.has_companion? %>
  Companion Name: <%= @account.companion.first_name %>
  Eligible: <%= @account.companion.eligible? %>
  Ready: <%= @account.companion.ready_to_apply? %>
<% else %>
  No Companion
<% end %>

<%= @account.updated_at.in_time_zone("EST") %>