<% cache(@airports) do %>
  <%
     array_for_typeahead = @airports.pluck(:id, :code, "parents_destinations.name").map do |airport|
      id = airport[0]
      code = airport[1]
      parent_name = airport[2]
      { id: id, code: code, name: "#{parent_name} (#{code})", tokens: [code, parent_name, "#{parent_name} (#{code})"] }
    end
  %>
  <%= raw(array_for_typeahead.to_json) %>
<% end %>
