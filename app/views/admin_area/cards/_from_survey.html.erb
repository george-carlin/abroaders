<table id="admin_person_cards_from_survey" class="table table-striped">
  <thead>
    <th>ID</th>
    <th>Name</th>
    <th>Status</th>
    <th>Opened</th>
    <th>Closed</th>
  </thead>
  <tbody>
    <% present_each cards do |card| %>
      <%= content_tag_for :tr, card do %>
        <td><%= card.product_identifier %></td>
        <td><%= card.product_name %></td>
        <td class="card_status"><%= card.status %></td>
        <td class="card_opened_at"><%= card.opened_at || '-' %></td>
        <td class="card_closed_at"><%= card.closed_at || '-' %></td>
      <% end # content_tag_for %>
    <% end %>
  </tbody>
</table>

