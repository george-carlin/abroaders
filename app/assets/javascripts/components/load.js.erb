//= require_self
//= require react_ujs

(function (w) {
  window.React     = require("react");
  window.ReactDOM  = require("react-dom");

  window.CardAccountStatuses = [
    <%= CardAccount.statuses.keys.map { |s| "'#{s}'" }.join(",") %>
  ]

  // We don't need to require *every* React component in this file, just the
  // ones that are required at the 'top level' by react_ujs
  w.CardAppliedActions        = require("./CardAppliedActions");
  w.CardRecommendationActions = require("./CardRecommendationActions");
  w.Typeahead                 = require("./Typeahead");
  w.TravelPlanForm            = require("./travel_plans/form/TravelPlanForm");
}(window));
